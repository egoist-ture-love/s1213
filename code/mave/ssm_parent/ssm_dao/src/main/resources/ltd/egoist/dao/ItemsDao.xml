<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ltd.egoist.dao.ItemsDao">

    <select id="findById" resultType="ltd.egoist.pojo.Items" parameterType="int">
        select * from items where id =#{id}
    </select>
    <select id="findAll" resultType="ltd.egoist.pojo.Items" >
        select * from items
    </select>
</mapper>